{
  "name": "@jothsa/stylelint-config-idiomatic-order",
  "version": "0.1.0",
  "description": "stylelint + idiomatic-css = ❤️",
  "scope": "@jothsa",
  "main": "index.js",
  "type": "commonjs",
  "dependencies": {
    "stylelint-order": "^6.0.4"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.5",
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "stylelint": ">=11"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "node test/test.js",
    "lint:es": "eslint .",
    "lint:prettier": "prettier --cache --plugin-search-dir . --check .",
    "lint": " pnpm lint:prettier; pnpm lint:es",
    "format:es": "eslint --fix",
    "format:prettier": "prettier --cache --plugin-search-dir . --write .",
    "format": "pnpm format:prettier; pnpm format:es;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jothsa/stylelint-config-idiomatic-order.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config",
    "idomatic-css",
    "order"
  ],
  "author": "Mario Uher <uher.mario@gmail.com> and Jothsa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jothsa/stylelint-config-idiomatic-order/issues"
  },
  "homepage": "https://github.com/jothsa/stylelint-config-idiomatic-order#readme"
}
